{
  "form":{
    "Parent":{
      "type":"text",
      "maxlength": 8,
      "pattern": "[a-zA-Z]*",
      "validation":["noNumerals","size"]
    },
    "Name":{
      "type":"text",
      "maxlength": 8,

      "validation": ["noNumerals","necessary","size"]
    },
    "Address":{
      "type": "text",
      "maxlength": 8,
      "validation": ["numerals"]
    },


    "Date":
    {
      "type": "date"
      "validation":[]
    },

    "Email":
    {
      "type": "email",
      "validation": ["necessary","email"]
    },

    "Password":
    {
      "type": "password",
      "pattern": "(?=.*\d)(?=.*[a-z])(?=.*[A-Z]).{6,}",
      "validation": ["necessary","password"]
    },

    "Age":
    {
      "type": "number",
      "validation":[],
      "min": 5,
      "max": 199
    },

    "PinCode":
    {
      "type": "text",
      "validation": [],
      "pattern": "[0-9]{6}"
    }

  }

}